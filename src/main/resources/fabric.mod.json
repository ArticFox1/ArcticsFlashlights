{
  "schemaVersion": 1,
  "id": "mod_id",
  "version": "${version}",
  "name": "Arctic's Flashlights",
  "description": "A mod that adds realistic flashlights to Minecraft. Will go well in a horror modpack.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/mod_id/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.ArcticFox.flash.mod_id.client.MainDataGenerator"
    ],
    "client": [
      "net.ArcticFox.flash.mod_id.client.MainClient"
    ],
    "main": [
      "net.ArcticFox.flash.mod_id.Main"
    ]
  },
  "mixins": [
    "mod_id.mixins.json",
    {
      "config": "mod_id.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
